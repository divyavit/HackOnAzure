doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    title LineUp
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css')
    link(href='https://fonts.googleapis.com/css?family=Dancing Script', rel='stylesheet')
    link(href='https://fonts.googleapis.com/css2?family=Lato:wght@300&family=Montserrat:wght@600&display=swap', rel='stylesheet')
    style
      include ../css/timeslot.css
    
  body
    // partial:index.partial.html
    section.search-form
      h1.heading_style choose time slot to analyze bookings
      form(action='/owner/'+id, method='post', name='search', role='search')
        p.inp-wrap.search-wrap
        p.inp-wrap.cat-wrap
          label.grid-20(for='categories')
          select#categories.grid-80(name='search_categories')
            option(value="10:00 to 12:00", selected='') 10:00 to 12:00
            option(value="12:00 to 14:00", selected='') 12:00 to 14:00
            option(value="14:00 to 16:00", selected='') 14:00 to 16:00
            option(value="16:00 to 18:00", selected='') 16:00 to 18:00
            option(value="18:00 to 20:00", selected='') 18:00 to 20:00
            option(value="20:00 to 22:00", selected='') 20:00 to 22:00
        p.inp-wrap.submit-wrap
          button.grid-100.btn
            span.search-icon-container
              // ?xml version="1.0" encoding="utf-8"?
              // Generated by IcoMoon.io
              svg#search-icon(version='1.1', xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', width='768', height='768', viewBox='0 0 768 768')
                g#icomoon-ignore
