doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    title analyze
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css')
    style
      include ../css/analyze.css
  body
    section  
      h1 Customers
      .tbl-header
        table(cellpadding='0', cellspacing='0', border='0')
          thead
            tr
              th Customer Id
              th status
      .tbl-content
        table(cellpadding='0', cellspacing='0', border='0')
          tbody
            each item in list
              tr
                td=item.id
                td=item.status 
        
    script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script
      include ../scripts/analyzescript.js
